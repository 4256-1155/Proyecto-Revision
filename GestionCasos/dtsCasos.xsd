                    <CommandText>SELECT        tRevision.Consecutivo, tRevision.Fecha, tInstitucion.Codigo, tInstitucion.Nombre, tRecepcion.Recepcion AS Recepción, tPersona.Cedula, tPersona.NombreCompleto, tRevision.Comentario, tRevision.Observacion, 
